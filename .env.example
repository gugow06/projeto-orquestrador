# Configuração das APIs de IA Generativa
# Copie este arquivo para .env.local e adicione suas chaves reais

# Google Gemini API Key
# Obtenha em: https://ai.google.dev/
NEXT_PUBLIC_GEMINI_API_KEY=sua_chave_gemini_aqui

# Groq API Key  
# Obtenha em: https://console.groq.com/
NEXT_PUBLIC_GROQ_API_KEY=sua_chave_groq_aqui

# Configurações opcionais
NEXT_PUBLIC_DEFAULT_AI_PROVIDER=gemini
NEXT_PUBLIC_APP_NAME="Orquestrador de Dados"
NEXT_PUBLIC_APP_VERSION="1.0.0"